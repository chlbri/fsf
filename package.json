{
  "name": "@bemedev/fsf",
  "version": "0.3.1",
  "description": "A libray for finite state functions",
  "author": {
    "name": "chlbri",
    "email": "bri_lviu@icloud.com",
    "url": "https://bemedev.vercel.app"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "keywords": [
    "statechart",
    "nodejs",
    "typescript",
    "javascript",
    "fsm",
    "finite-state-machine",
    "if",
    "context",
    "data",
    "server",
    "statechart machine",
    "machine"
  ],
  "files": [
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "config": "pnpm install",
    "rollup": "rollup -c",
    "prepare": "husky install",
    "updateDependencies": "pnpm update --latest",
    "build": "rm -rf lib && pnpm run rollup",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "remove": "rm -rf node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "prepublishOnly": "pnpm run build",
    "p-q": "pretty-quick --staged --pattern \"**/*.*(ts|tsx)\""
  },
  "devDependencies": {
    "@bemedev/npm-publish": "0.0.1-pnpm2",
    "@types/faker": "^6.6.9",
    "@types/lodash.clonedeep": "^4.5.7",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "add": "^2.0.6",
    "esbuild": "^0.16.6",
    "eslint": "^8.29.0",
    "faker": "^6.6.6",
    "husky": "^8.0.2",
    "nanoid": "^4.0.0",
    "onchange": "^7.1.0",
    "pnpm": "^7.18.2",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.4",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.4.0",
    "shelljs": "^0.8.5",
    "tsd": "^0.25.0",
    "typescript": "^4.9.4",
    "vite": "3.2.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "0.25.2"
  },
  "repository": "https://github.com/chlbri/fstate.git",
  "dependencies": {
    "@bemedev/x-guard": "^0.1.0",
    "lodash.clonedeep": "^4.5.0"
  }
}
