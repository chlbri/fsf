{
  "name": "@bemedev/fsf",
  "version": "1.0.0",
  "description": "A library for finite state functions",
  "author": {
    "name": "chlbri",
    "email": "bri_lvi@icloud.com",
    "url": "https://bemedev.vercel.app"
  },
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.js",
  "keywords": [
    "fsm",
    "if",
    "state machine"
  ],
  "files": [
    "lib",
    "public"
  ],
  "license": "MIT",
  "size-limit": [
    {
      "path": "lib/*.js",
      "limit": "10 KB"
    },
    {
      "path": "lib/*.cjs",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "build": "pnpm run rimraf lib && pnpm run rollup",
    "ci": "pnpm run config:off && pnpm run lint && pnpm run test",
    "ci:admin": "pnpm run remove && pnpm run upgrade && pnpm run ci",
    "config": "pnpm install",
    "config:off": "pnpm install --offline",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "p-q": "pretty-quick --pattern \"**/*.*(ts|tsx)\"",
    "posttest": "pnpm run size && npm run prettier && pnpm run p-q",
    "prepare": "husky",
    "prepublishOnly": "pnpm run lint && pnpm run build",
    "pretest": "pnpm run build",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "remove": "pnpm run rimraf -rf node_modules pnpm-lock.yaml",
    "rimraf": "pnpm dlx rimraf",
    "rinit": "pnpm run remove && pnpm run config:off",
    "rollup": "rollup -c",
    "size": "size-limit",
    "test": "pnpm run test:coverage --no-coverage",
    "test:coverage": "vitest run",
    "test:only": "vitest run ./src/monad.test.ts",
    "upgrade": "pnpm dlx @bemedev/upgrade-npm-package"
  },
  "devDependencies": {
    "@bemedev/decompose": "^1.3.1",
    "@bemedev/return-data": "^0.3.2",
    "@bemedev/rollup-config": "^0.2.2",
    "@bemedev/vitest-alias": "^0.0.3",
    "@bemedev/vitest-exclude": "^0.1.1",
    "@bemedev/vitest-extended": "^1.5.3",
    "@bemedev/zod-extended": "^0.1.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/object.omit": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^3.2.4",
    "dayjs": "^1.11.18",
    "dequal": "^2.0.3",
    "edit-json-file": "^1.8.1",
    "eslint": "^9.37.0",
    "faker": "^6.6.6",
    "glob": "^11.0.3",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "immer": "^10.1.3",
    "mock-stdin": "^1.0.0",
    "mongodb": "^6.20.0",
    "nanoid": "^5.1.6",
    "object.omit": "^3.0.0",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-tsc-alias": "^1.1.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "shelljs": "^0.10.0",
    "size-limit": "^11.2.0",
    "ts-deepmerge": "^7.0.3",
    "ts-node": "^10.9.2",
    "tsd": "^0.33.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "zod": "^4.1.12"
  },
  "repository": "https://github.com/chlbri/fstate.git",
  "dependencies": {
    "@bemedev/x-guard": "^0.2.0",
    "deepmerge": "^4.3.1"
  }
}
