{
  "name": "gitpod-typescript",
  "version": "0.0.1",
  "description": "https://github.com/chlbri/fstate.git",
  "author": "chlbri <bri_lvi@icloud.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "build:-test": "yarn run tsc && yarn run lint",
    "build": "yarn run test && yarn run build:-test",
    "test": "jest",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "yarn run prettier && yarn run eslint --fix",
    "rm": "./node_modules/.bin/rimraf -rf node_modules yarn.lock",
    "rinit": "yarn run rm && yarn cache clean && yarn run yarn",
    "publish": "yarn run rm && yarn run clean && yarn run yarn",
    "clean": "yarn cache clean",
    "yarn": "yarn install",
    "git-push": "git push --tags"
  },
  "devDependencies": {
    "@core_chlbri/test": "https://github.com/chlbri/core-test.git#9ca4d74533114ce8844c75c395824f6c9161fa7d",
    "@types/faker": "^6.6.9",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "dayjs": "^1.10.7",
    "edit-json-file": "^1.7.0",
    "eslint": "^8.8.0",
    "faker": "^6.6.6",
    "immer": "^9.0.12",
    "jest": "^27.5.0",
    "jest-cucumber": "^3.0.1",
    "jest-extended": "^2.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-jest": "^27.1.3",
    "tsd": "^0.19.1",
    "typescript": "^4.5.5",
    "nanoid": "^3.2.0"
  },
  "dependencies": {
    "dequal": "^2.0.2"
  },
  "peerDependencies": {
    "nanoid": "^3.2.0"
  },
  "repository": "https://github.com/chlbri/fstate.git"
}
